package com.service.employee.service;

import java.util.List;

import javax.persistence.Entity;

import com.service.employee.utils.EmployeeUtils;

@Entity
public interface EmployeeService {

	EmployeeUtils createEmployee(EmployeeUtils empUtil);

	EmployeeUtils findEmpByEmail(String email);

	List<EmployeeUtils> findEmpsByOrgEmail(String email);

	Employee deleteEmpByEmail(String email);

}
